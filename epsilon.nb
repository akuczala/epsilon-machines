Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"EquivalenceIndices", "[", 
    RowBox[{"list_", ",", "f_"}], "]"}], ":=", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"matchingPairs", ",", "graph", ",", "components"}], "}"}], ",", 
     RowBox[{
      RowBox[{"matchingPairs", " ", "=", " ", 
       RowBox[{"Select", "[", 
